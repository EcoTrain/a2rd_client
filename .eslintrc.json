{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "no-useless-catch": "off",
        "no-unused-vars": "warn",
        "react/prop-types": "warn"
    }
}